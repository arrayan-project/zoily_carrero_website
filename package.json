    "react-device-detect": "^2.2.3",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-google-recaptcha": "^3.1.0",
    "react-helmet-async": "^2.0.5",
    "react-intersection-observer": "^9.16.0",
